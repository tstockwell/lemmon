<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
	<application>emorning</application>
	<version>0-1-24</version>
	
	<system-properties>
	
		<!-- Configure java.util.logging -->
		<property name="java.util.logging.config.file" value="WEB-INF/conf/logging.properties"/>
	
		<!--  
		These properties need to be set in order to get 
		org.apache.commons.vfs.util.Os to load 
		-->
		<property name="os.name" value="unix"/>
		<property name="os.arch" value="x86"/>
		<property name="os.version" value="0.0.0"/>
		
	</system-properties>


</appengine-web-app>